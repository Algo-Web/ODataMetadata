{
  "name": "algo-web/o-data-metadata",
  "description": "Package responsible for OData feed metadata description",
  "version": "0.1.4",
  "type": "library",
  "keywords": [
    "OData",
    "metadata",
    "POData"
  ],
  "homepage": "https://github.com/Algo-Web/ODataMetadata",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Goodwin",
      "email": "cyberia.resurrection@gmail.com",
      "homepage": "http://www.example.org",
      "role": "Developer"
    },
    {
      "name": "Chris Harris",
      "email": "c.harris@communicationinnovations.com.au",
      "homepage": "http://www.example.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "myclabs/php-enum": "dev-master",
    "ext-dom": "*",
    "ext-xmlwriter": "*",
    "ext-xmlreader": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "autoload": {
    "psr-4": {
      "AlgoWeb\\ODataMetadata\\": "src/"
    },
    "files": ["src/polyfill.php"]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
