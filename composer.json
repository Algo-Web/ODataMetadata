{
  "name": "bim/o-data-metadata",
  "description": "Package responsible for OData feed metadata description",
  "type": "library",
  "keywords": [
    "OData",
    "metadata",
    "POData"
  ],
  "homepage": "https://github.com/Algo-Web/ODataMetadata",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Goodwin",
      "email": "cyberia.resurrection@gmail.com",
      "homepage": "http://www.example.org",
      "role": "Developer"
    },
    {
      "name": "Chris Harris",
      "email": "c.harris@communicationinnovations.com.au",
      "homepage": "http://www.example.org",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kirill533/serializer.git",
      "no-api": true
    }
  ],
  "require": {
    "php": "~5.6|~7.0|~7.1",
    "illuminate/support": "^4.0|^5.1",
    "bim/serializer": "1.8.2"
  },
  "require-dev": {
    "brianium/paratest": "0.15.*",
    "phpspec/phpspec": "^3.0",
    "squizlabs/php_codesniffer": "~2.3",
    "fiunchinho/phpunit-randomizer": "2.0.*",
    "mockery/mockery": "0.9.*|^1.0@dev",
    "phpunit/phpunit": "^5.6",
    "php-coveralls/php-coveralls": "*"
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "autoload": {
    "psr-4": {
      "AlgoWeb\\ODataMetadata\\": "src/"
    },
    "classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {

  },
  "config": {
    "sort-packages": true
  }
}
